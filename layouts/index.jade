extends layout.jade
block content
  ul.nav
    li.nav__item
      a(href="/about") about
    li.nav__item
      a(href="https://www.facebook.com/bemorechildish/") facebook
    li.nav__item
      a(href="https://soundcloud.com/bemorechildish") soundcloud
    li.nav__item
      a(href="/#") newsletter
  .sticker.sticker--bottom-left.sticker--responsive-zoom
    img(src="/img/childsplay.png", title="childsplay")
  .sticker.sticker--bottom-right.sticker--responsive-zoom
    img(src="/img/tasty_treats.png", title="tasty treats for the kids, all flavors welcome!!")
  .bg.bg--pink
    ul.grid
      each file in lasagna_files
        li.grid__item
          a.track(href=file.link, title=file.track_name)
            img.track__cover(src=file.cover_url)
        li.grid__item
          a.track(href=file.link, title=file.track_name)
            img.track__cover(src=file.cover_url)
        li.grid__item
          a.track(href=file.link, title=file.track_name)
            img.track__cover(src=file.cover_url)
  != contents

